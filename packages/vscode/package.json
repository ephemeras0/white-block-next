{
  "name": "@white-block/vscode",
  "description": "vscode extension for white-block",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kythuen/white-block.git"
  },
  "bugs": {
    "url": "https://github.com/kythuen/white-block/issues"
  },
  "displayName": "WhiteBlock",
  "icon": "logo.png",
  "publisher": "ephemeras",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "config": "tsx scripts/config.ts",
    "test": "echo \"@white-block/vscode ignore\"",
    "test:coverage": "echo \"@white-block/vscode coverage ignore\""
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@vscode/vsce": "^2.31.0",
    "@white-block/types": "workspace:*",
    "fast-glob": "^3.3.1"
  },
  "dependencies": {
    "radash": "^12.1.0"
  }
}
